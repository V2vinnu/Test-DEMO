pipeline {
  agent any 
        stages {
            stage('git checkout') {
                steps{
               git branch: 'main', url: 'https://github.com/V2vinnu/Test-DEMO.git'
            }
            }
        
      
            stage('maven build'){
            
            steps{
                
                script{
                    
                    sh 'mvn clean install'
                }
            }
        }
    }
}
