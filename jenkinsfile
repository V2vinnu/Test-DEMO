pipeline {
  agent any 
        stages {
            stage('git checkout') {
                steps{
               git branch: 'main', url: 'https://github.com/V2vinnu/Test-DEMO.git'
            }
            }
        }
      
            stage('Unit test') {
                steps{
                    sh 'mvn test'
               
            }
            }
        }
