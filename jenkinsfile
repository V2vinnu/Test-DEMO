pipeline {
  agent any 
        stages {
            stage('git checkout') {
                steps{
               git branch: 'main', url: 'https://github.com/V2vinnu/Test-DEMO.git'
            }
            }
        
      
            stage('Maven Unit test'){
            
            steps{
                
                script{
                    
                    sh 'mvn test'
                }
            }
        }

        stage('maven integration test'){
            steps{
                script{
                    sh 'mvn verify -DskipUnitTest'
                }
            }
        }

        stage('maven build'){
            steps{
                script{
                    sh 'mvn clean install'
                }
            }
        }
    }
}
